{
  "manifest_version": 3,
  "name": "Cookie Editor",
  "version": "2.1",
  "description": "Simple Cookie Editor. Create, Delete, Edit, Copy cookies. Will be improved later on.",
  "permissions": [
    "cookies",
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "main.html"
  },
  "icons": {
    "128": "icons/cookie_icon.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
